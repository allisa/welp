<% results.forEach(place =>  {%>
<div class="card">
  <h3>
    <%= place.name %>
  </h3>
  <img src="<%= place.image %>" alt="image" />
  <p>Specialties:
    <%= place.categories %>
  </p>
  <p>Rating:
    <%= place.rating %>
  </p>
  <p>Open Now:
    <%= place.hours %>
  </p>
  <p><a href="<%= place.url %>">External Link</a></p>
  <form method="POST" action="/place">
    <button value="Add">Let's go here!</button>
  </form>
</div>
<% }) %>